<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="WordPressTestSuite" verbose="1">
    <listeners>
        <listener class-name="aqajava.listeners.TestSuiteListener"/>
    </listeners>
    <test name="WordPressTest">
        <classes>
            <class name="aqajava.tests.AdminPermissionsTest"/>
            <class name="aqajava.tests.AuthenticationTest"/>
            <class name="aqajava.tests.AuthorPermissionsTest"/>
            <class name="aqajava.tests.ContributorPermissionsTest"/>
            <class name="aqajava.tests.EditorPermissionsTest"/>
            <class name="aqajava.tests.VisitorCreateCommentTest"/>
        </classes>
    </test>
</suite>
